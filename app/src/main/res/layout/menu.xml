<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.example.pc_render.mobpdi.Menu"
    android:background="@drawable/b_123"
    xmlns:ads="http://schemas.android.com/apk/res-auto">


    <ScrollView
        android:layout_width="match_parent"
        android:layout_height="match_parent">

    <LinearLayout


        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:gravity="center"
        android:orientation="vertical">
        <com.google.android.gms.ads.AdView

            android:id="@+id/adview"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentTop="true"
            android:layout_centerHorizontal="true"
            android:layout_marginTop="32dp"
            ads:adSize="BANNER"
            ads:adUnitId="ca-app-pub-4356985467396850/8805316003" />

        <com.google.android.gms.ads.AdView  
            android:id="@+id/adview1"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerInParent="true"
            ads:adSize="BANNER"
            ads:adUnitId="ca-app-pub-4356985467396850/8805316003" />
        <TextView
            android:textColor="#fff"
            android:id="@+id/textView"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginBottom="40dp"
            android:text="Pengemis Jalanan"
            android:textAlignment="center"
            android:textAllCaps="true"
            android:textSize="19sp"
            android:textStyle="bold" />

        <WebView
            android:layout_gravity="center_horizontal"
            android:id="@+id/webview"
            android:layout_width="200dp"
            android:layout_height="200dp"
            android:gravity="center"

            />


        <Button
            android:paddingLeft="16dp"
            android:paddingRight="16dp"
            android:drawableLeft="@drawable/ic_create_white_24dp"
            android:layout_marginLeft="60dp"
            android:layout_marginRight="60dp"
            android:background="@drawable/btn"
            android:layout_marginTop="30dp"
            android:id="@+id/idtuyul"
            android:layout_width="match_parent"
            android:layout_height="40dp"
            android:gravity="center"
            android:textColor="#fff"
            android:text="Ganti ID Admob"

            />

        <Button
            android:paddingLeft="16dp"
            android:paddingRight="16dp"
            android:drawableLeft="@drawable/ic_play_arrow_white_24dp"
            android:layout_marginLeft="60dp"
            android:layout_marginRight="60dp"
            android:layout_marginTop="4dp"
            android:background="@drawable/btn"
            android:textColor="#fff"
            android:id="@+id/interval"
            android:layout_width="match_parent"
            android:layout_height="40dp"
            android:gravity="center"
            android:text="Giling Uang" />

        <Button
            android:paddingLeft="16dp"
            android:paddingRight="16dp"
            android:drawableLeft="@drawable/ic_language_white_24dp"
            android:layout_marginLeft="60dp"
            android:layout_marginRight="60dp"
            android:layout_marginTop="4dp"
            android:background="@drawable/btn"
            android:textColor="#fff"
            android:id="@+id/vpn"
            android:layout_width="match_parent"
            android:layout_height="40dp"
            android:gravity="center"
            android:text="Buka VPN Full Pro" />

        <Button
            android:paddingLeft="16dp"
            android:paddingRight="16dp"
            android:drawableLeft="@drawable/ic_android_white_24dp"
            android:layout_marginLeft="60dp"
            android:layout_marginRight="60dp"
            android:layout_marginTop="4dp"
            android:background="@drawable/btn"
            android:textColor="#fff"
            android:id="@+id/apk"
            android:layout_width="match_parent"
            android:layout_height="40dp"
            android:gravity="center"
            android:text="Download Apk Editor Pro" />

        <Button
            android:paddingLeft="16dp"
            android:paddingRight="16dp"
            android:drawableLeft="@drawable/ic_error_white_24dp"
            android:layout_marginLeft="60dp"
            android:layout_marginRight="60dp"
            android:layout_marginTop="4dp"
            android:background="@drawable/btn"
            android:textColor="#fff"
            android:id="@+id/info"
            android:layout_width="match_parent"
            android:layout_height="40dp"
            android:gravity="center"
            android:text="Informasi"
            android:layout_marginBottom="70dp"/>

    </LinearLayout>
    </ScrollView>
</android.support.constraint.ConstraintLayout>
